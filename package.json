{
  "name": "@opuscapita/react-markdown",
  "version": "0.0.1-beta.0",
  "description": "Start write new project with no effort.",
  "scripts": {
    "npm-build": "rimraf lib && cross-env NODE_ENV=production webpack --config ./config/webpack.config.js --hide-modules",
    "link-mode": "cross-env NODE_ENV=link webpack --config ./config/webpack.config.js",
    "lint": "eslint src/client",
    "upload-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "rimraf ./.nyc_output ./coverage && cross-env nyc --reporter=lcov --reporter=text mocha --require config/test/mocha-setup.js --recursive src/**/*.spec.js --require ignore-styles",
    "publish-release": "npm run npm-build && npm-publish --release",
    "start": "showroom-scan src && babel-node --presets es2015,stage-0 www/index",
    "gh-pages-generate": "showroom-scan src && sh scripts/gh-pages/generate.sh",
    "gh-pages-deploy": "npm run gh-pages-generate && sh scripts/gh-pages/deploy.sh"
  },
  "license": "Apache-2.0",
  "repository": "OpusCapita/react-markdown",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^0.14.8 || ^15.4.2",
    "react-dom": "^0.14.8 || ^15.4.2"
  },
  "devDependencies": {
    "@opuscapita/npm-scripts": "1.0.8",
    "@opuscapita/opuscapita-ui": "1.0.2-beta.4",
    "@opuscapita/react-showroom-client": "1.2.10",
    "@opuscapita/react-showroom-server": "1.3.0",
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "compression": "1.6.2",
    "coveralls": "2.13.1",
    "cross-env": "5.0.1",
    "css-loader": "0.26.2",
    "enzyme": "2.9.1",
    "eslint": "3.17.0",
    "eslint-config-opuscapita": "1.0.7",
    "eslint-plugin-react": "6.10.0",
    "express": "4.15.1",
    "file-loader": "0.10.1",
    "ignore-styles": "5.0.1",
    "istanbul": "0.4.5",
    "jsdom": "9.12.0",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "mocha": "3.4.2",
    "mocha-junit-reporter": "1.13.0",
    "nodemon": "1.11.0",
    "nyc": "11.1.0",
    "postcss-loader": "1.3.3",
    "postcss-modules": "0.6.4",
    "progress-bar-webpack-plugin": "1.10.0",
    "raw-loader": "0.5.1",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-test-renderer": "15.6.1",
    "rimraf": "2.6.1",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "1.10.1",
    "write-file-webpack-plugin": "3.4.2"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "markdown-it": "8.3.1",
    "markdown-it-deflist": "2.0.2",
    "markdown-it-emoji": "1.4.0",
    "markdown-it-footnote": "3.0.1",
    "markdown-it-ins": "2.0.0",
    "markdown-it-mark": "2.0.0",
    "markdown-it-sub": "1.0.0",
    "markdown-it-sup": "1.0.0",
    "prismjs": "1.6.0",
    "prop-types": "15.5.10",
    "query-string": "4.3.4",
    "react-bootstrap": "0.31.0",
    "slate": "0.20.6",
    "slate-markdown-serializer": "0.1.5"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/client/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "**/*.SCOPE*.js",
      "**/demo/**/*.js"
    ],
    "check-coverage": true,
    "all": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80
  }
}
